{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        {
            "label": "dev appengine",
            "command": "dev_appserver.py ./demo/appengine/app.yaml",
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "type": "shell",
            "problemMatcher": [
                "$go"
            ]
        },
        {
            "label": "dev jekyll",
            "command": "jekyll serve --source ./docs",
            "group": "build",
            "type": "shell",
            "problemMatcher": [
                "$go"
            ]
        }
    ]
}